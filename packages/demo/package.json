{
  "name": "logicflow-vue-demo",
  "version": "0.3.0",
  "description": "Logic Flow Vue Demo",
  "private": true,
  "scripts": {
    "all": "pnpm clean && pnpm build",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
