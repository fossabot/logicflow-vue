    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "@vitejs/plugin-vue": "^2.3.2",
    "cypress": "^9.6.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.72.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "sharp": "^0.30.4",
    "typescript": "^4.6.4",
    "unplugin-vue-components": "0.19.5",
